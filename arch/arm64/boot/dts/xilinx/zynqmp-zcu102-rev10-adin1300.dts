#include "zynqmp-zcu102-rev1.0.dts"

&gem0 {
	status = "okay";
	phy-mode = "gmii";
	phy-handle = <&ethernet_gem0_phy0>;

	mdio {
		#address-cells = <1>;
		#size-cells = <0>;

		ethernet_gem0_phy0: ethernet-phy@0 {
			phy-mode-internal = "rgmii";
			reg = <0>;
		};
	};
};

&gem1 {
	status = "disabled";
};
